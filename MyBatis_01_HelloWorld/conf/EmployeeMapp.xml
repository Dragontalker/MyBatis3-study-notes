<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
	namespace: 名称空间
	id: 唯一标识
	resultType: 返回值类型
-->
<mapper namespace="com.dragontalker.mybatis.EmployeeMapper">
	<select id="selectEmp" resultType="com.dragontalker.mybatis.bean.Employee">
		select * from tbl_employee where id = #{id}
	</select>
</mapper>